{
  "name": "fynt/eloquent-history",
  "description": "Save a history of actions on your Eloquent models.",
  "tags": ["laravel", "eloquent", "history"],
  "authors": [
    {
      "name": "Ryan",
      "email": "ryan@fynt.ca"
    },
    {
      "name": "Keith",
      "email": "keith@fynt.ca"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "illuminate/auth": "4.2.*",
    "illuminate/config": "4.2.*",
    "illuminate/database": "4.2.*",
    "illuminate/support": "4.2.*",
    "mockery/mockery": "0.9.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "autoload": {
    "classmap": [
      "src/migrations"
    ],
    "psr-0": {
      "Fynt\\EloquentHistory\\": "src/"
    }
  },
  "minimum-stability": "stable"
}
