{
    "name": "fynt/eloquent-history",
    "description": "Save history of user actions on your Eloquent models",
    "authors": [
        {
            "name": "Ryan",
            "email": "ryan@fynt.ca"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "fynt/eloquent-versions": "*"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Fynt\\EloquentHistory\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
